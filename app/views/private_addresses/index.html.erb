<p id="notice"><%= notice %></p>

<h1>Private Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Street name</th>
      <th>Other information</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @private_addresses.each do |private_address| %>
      <tr>
        <td><%= private_address.street_name %></td>
        <td><%= private_address.other_information %></td>
        <td><%= link_to 'Show', private_address %></td>
        <td><%= link_to 'Edit', edit_private_address_path(private_address) %></td>
        <td><%= link_to 'Destroy', private_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Private Address', new_private_address_path %>
